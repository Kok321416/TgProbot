# Production Environment Configuration
# Скопируйте этот файл в .env на сервере и заполните реальными данными

# Telegram Bot Configuration
TOKEN=your_production_bot_token_here

# Email Configuration
EMAIL_HOST=smtp.yandex.ru
EMAIL_PORT=465
EMAIL_USER=your_production_email@yandex.ru
EMAIL_PASSWORD=your_production_email_password
EMAIL_TO=your_production_recipient@yandex.ru

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=/app/logs/tgprobot.log
